{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "dependsOn": ["//build-tsup-config"],
      "cache": false,
      "persistent": true
    },
    "//build-tsup-config": {
      "dependsOn": ["@org/tsup-config#build"]
    },
    "clean": {
      "cache": false
    }
  }
}
